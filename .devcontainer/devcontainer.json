{
	"runArgs": ["--init"],
	"build": {
		"dockerfile": "../Dockerfile",
		"context": "..",
	},
	"extensions": [
		"vscjava.vscode-java-pack"
	],
	// In order to use GUI tool of the host computer.
	/// Window management tool
	"features": {
		"desktop-lite": "latest",
	},
	//// Set <Your Private IP Address>:<Display Number>.<Screen Number>
	"remoteEnv": {
		"DISPLAY": "192.168.33.1:0.0"
	},
}